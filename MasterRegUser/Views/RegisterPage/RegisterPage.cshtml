<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />

    <title>@ViewData["Title"] - Document Management System</title>
    <link href="~/css/bootstrap.min.css" type="text/css" rel="stylesheet" />
    <link href="~/css/bootstrap.css" type="text/css" rel="stylesheet" />
    <link href="~/css/font-awesome.css" type="text/css" rel="stylesheet" />
    <link href="~/css/customcss/custom.css" type="text/css" rel="stylesheet" />
    <script src="~/script/jquery-3.3.1.min.js" type="text/javascript"></script>
    <script src="~/script/customscriptj.js"></script>
    </head>
    <body>
<div id="Wrapper">
    <div id="Bungkus-Login">
        <div class="Kiri">
                <img src="~/image/ts61hf08szppzlocznxew.png" />
    </div>
    <div class="Kanan">

                <ul>

                    <li>
                        <h2><label class="title-login">  Register New Account </label></h2>
                    </li>
                    <hr />
                    <li>
                        <label> Full Name</label>
                    </li>
                    <li>
                        <input type="text" autocomplete="off" id="txtFullName" class="form-control" />
                    </li>
                    <li>
                        <label> Email</label>
                    </li>
                    <li>
                        <input type="text" autocomplete="off" id="txtEmail" class="form-control" />
                    </li>
                    <li>
                        <label> Password</label>
                    </li>
                    <li>
                        <input type="password" autocomplete="off" id="txtpass" class="form-control" />
                    </li>
                    <li>
                        <label> ReType Password</label>
                    </li>
                    <li>
                        <input type="password" autocomplete="off" id="txtRepass" class="form-control" />
                    </li>
                    <li>
                        <label> Phone Number</label>
                    </li>
                    <li>
                        <input type="text" autocomplete="off" id="txtNumberPhone" class="form-control" />
                    </li>
                    <li>
                        <label> Nationality</label>
                    </li>
                    <li>
                        <select id="dpNationality" class="form-control">
                            <option value="Indonesian">Indonesian</option>
                            <option value="Malaysian">Malaysian</option>
                            <option value="Singaporean">Singaporean</option>
                            <option value="Indian">Indian</option>
                            <option value="USA">USA</option>
                            <option value="Other">Other</option>

                        </select>
                    </li>
                    <li>
                        <input type="text" autocomplete="off"  id="txtNationality" placeholder="Please Fill Your Nationality" class="form-control" />
                    </li>
                    <li>
                        <button type="button" id="btnSubmitAccount" class="btn btn-primary"><i class="fa fa-key">Submit New Account</i></button>
                   
                        <button type="button" id="btnLogin" class="btn btn-default"><i class="fa fa-key">Login</i></button>
                    </li>
                </ul>

    </div>

        </div>
</div>
</body>
</html>
<script>
    $(document).ready(function () {
        var Nation = document.getElementById('txtNationality');
        Nation.style.display = 'none';
        $("#dpNationality").on("change", function () {
            CekDPNationality();
        });
       
        $("#btnLogin").click(function () {
            open("/MasterReg/LoginPage/LoginPage", "_self");

        });
        $("#btnSubmitAccount").click(function () {
            Register();
        });

    });
</script>